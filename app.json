{
  "expo": {
    "name": "Más con Menos - Gerentes",
    "slug": "MasConMenos-gerentes",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logos/icon.png",
    "scheme": "sicapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/logos/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#a93439"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Esta aplicación necesita acceso a la cámara para capturar fotos de comprobantes de pago.",
        "NSLocationWhenInUseUsageDescription": "Esta aplicación necesita acceso a la ubicación para registrar la posición donde se realizan los pagos."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logos/icon.png",
        "backgroundColor": "#a93439"
      },
      "package": "com.mcm_dev_01.MasConMenos_gerentes",
      "permissions": [
        "CAMERA",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logos/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ]
    ],
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "5eb82af2-2cd5-49ea-b4bd-77126472d555"
      }
    }
  }
}
